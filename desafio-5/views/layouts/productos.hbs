<table class="table table-dark">
  <thead>
    <tr>
      <th scope="col">ID</th>
      <th scope="col">NOMBRE</th>
      <th scope="col">PRECIO</th>
      <th scope="col">IMAGEN</th>
    </tr>
  </thead>
  <tbody>
    {{#each productos}}
      <tr>
        <th>{{this.id}}</th>
        <th scope="row">{{this.title}}</th>
        <th>{{this.price}}</th>
        <th><img src="{{this.thumbnail}}" /></th>
      </tr>
    {{/each}}
  </tbody>
</table>